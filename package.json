    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.7.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "commander": "^4.0.1",
    "date-fns": "^2.8.1",
