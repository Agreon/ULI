{
  "name": "juli",
  "description": "Jira Universal Log Importer",
  "repository": {
    "url": "https://github.com/Agreon/juli"
  },
  "version": "0.0.5",
  "main": "dist/command.js",
  "author": "Daniel Huth",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "yarn clean; tsc; cp src/command.js dist/command.js",
