{
  "rules": {
    "arrow-parens": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "object-shorthand-properties-first": false,
    "member-access": false,
    "no-implicit-dependencies": false,
    "array-type": false,
    "import-name": false,
    "no-floating-promises": true,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false
  }
}
